<!-- dashboard for signed in users, question-index as landing dashboard page  -->
<h1>User dashboard page</h1>

<!-- Profile button, WORKING DONT TOUCH -->
<form action="/users/:id" method="post">
    <button type='submit'>Profile</button>
</form>

<!-- question index -->
<form action='/users/questions' method="post">
    <input name="question-index" type='hidden' value='post'/>
    <button type='submit'>All question</button>
</form>

<!-- log out button  -->
<form action='/sessions/:id' method="post">
    <input name="_method" type='hidden' value='delete'/>
    <button type='submit'>Log Out</button>
</form>

<!-- back button -->
<FORM><INPUT ID="back-btn" Type="button" VALUE="Back" onClick="history.go(-1);return true;"></FORM>
